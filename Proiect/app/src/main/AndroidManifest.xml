<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proiect">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@mipmap/ic_people_foreground"
        android:label="@string/androidele_RentManager"
        android:roundIcon="@mipmap/ic_people_round"
        android:supportsRtl="true"
        android:theme="@style/AndroideleStyleLight">
        <activity android:name=".AddInvoiceActivity"></activity>
        <activity android:name=".AddUtilityActivity" />
        <activity android:name=".AddApartmentActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Rent Manager"
            android:theme="@style/AndroideleStyleLight" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AndroideleStyleLight" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AndroideleStyleLight">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>