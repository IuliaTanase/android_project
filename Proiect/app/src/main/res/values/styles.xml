<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="AndroideleStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/PaleVioletRed</item>
        <item name="colorPrimaryDark">@color/DarkSlateBlue</item>
        <item name="colorAccent">@color/DarkSlateBlue</item>
        <item name="background">@color/MistyRose</item>
        <item name="fontFamily">@font/coming_soon</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>